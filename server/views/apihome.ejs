<h1>api home</h1>

<% players.forEach((player) => { %>
    <li>
        <h4><%= player.fullname %></h4>
        <h4><%= player.firstname %></h4>
        <h4><%= player.lastname %></h4>
        <h4><%= player.email %></h4>
        <h4><%= player.mobile %></h4>
        <h4><%= player.note %></h4>
        <a href="/api/rosters/<%= player._id %>" class="btn btn-primary">More Details</a>
    </li>

<% }); %>